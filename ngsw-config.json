{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-warehouses",
      "urls": [
        "https://apispacesegure-0mzh.onrender.com/api/bodegasHome",
        "https://apispacesegure-0mzh.onrender.com/api/bodegas",
        "https://apispacesegure-0mzh.onrender.com/api/bodegas/*",
        "https://apispacesegure-0mzh.onrender.com/api/bodegasUsuario/*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "24h",
        "timeout": "5s"
      }
    },
    {
      "name": "api-locations",
      "urls": [
        "https://apispacesegure-0mzh.onrender.com/api/locations",
        "https://apispacesegure-0mzh.onrender.com/api/locations/*",
        "https://apispacesegure-0mzh.onrender.com/api/locationsSelect"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "24h",
        "timeout": "5s"
      }
    },
    {
      "name": "api-users",
      "urls": [
        "https://apispacesegure-0mzh.onrender.com/api/usuarios",
        "https://apispacesegure-0mzh.onrender.com/api/usuarios/*",
        "https://apispacesegure-0mzh.onrender.com/api/usuario/*",
        "https://apispacesegure-0mzh.onrender.com/api/usuariosSelect",
        "https://apispacesegure-0mzh.onrender.com/api/blockUsuario/*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "12h",
        "timeout": "5s"
      }
    }
  ]
}
